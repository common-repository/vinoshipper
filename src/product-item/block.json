{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "vinoshipper-injector/product-item",
	"version": "1.1.0",
	"title": "Product Item",
	"category": "vinoshipper",
	"icon": "admin-page",
	"description": "Display one product with image, description, and other information.",
	"keywords": [ "vs" ],
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "vinoshipper",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"render": "file:./render.php",
	"attributes": {
		"productId": {
			"type": "number",
			"default": null
		},
		"accountId": {
			"type": "number",
			"default": null
		},
		"cards": {
			"type": "string",
			"default": "list",
			"enum": [ "cards", "list" ]
		},
		"image": {
			"type": "boolean",
			"default": true
		},
		"descForce": {
			"type": "boolean",
			"default": false
		}
	}
}
